{
  "name": "dan-tranh-tablature",
  "version": "1.0.0",
  "description": "Dan Tranh Tablature with parallel development",
  "scripts": {
    "dev:original": "node server-original.js",
    "dev:v2": "node server-v2.js",
    "dev:v3": "node server-v3.js",
    "dev:all": "concurrently \"npm run dev:original\" \"npm run dev:v2\" \"npm run dev:v3\"",
    "dev:both": "concurrently \"npm run dev:original\" \"npm run dev:v2\"",
    "dev": "npm run dev:all",
    "dev:watch": "nodemon server.js",
    "test": "node test-server.js",
    "v3:import": "node v3/auto-import.js",
    "v3:import:watch": "node v3/auto-import.js --watch",
    "v3:process": "node v3/import-processor.js",
    "build": "mkdir -p docs && cp -r v3/* docs/ && echo 'GitHub Pages build complete - deploy from docs/ folder'"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "jsdom": "^27.0.0"
  }
}
